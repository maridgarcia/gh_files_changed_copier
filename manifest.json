{
  "manifest_version": 3,
  "name": "GitHub Files Changed Copier",
  "version": "1.0",
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "clipboardWrite"
  ],
  "host_permissions": [
    "https://github.com/*"
  ],
  "action": {
    "default_popup": "index.html"
  }
}
